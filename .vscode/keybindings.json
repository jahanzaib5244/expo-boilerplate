// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "shift+cmd+s",
    "command": "workbench.action.files.save"
  },
  {
    "key": "cmd+s",
    "command": "-workbench.action.files.save"
  },
  {
    "key": "cmd+s",
    "command": "saveAll"
  },
  {
    "key": "alt+cmd+s",
    "command": "-saveAll"
  },
  {
    "key": "cmd+-",
    "command": "composer.startComposerPrompt",
    "when": "composerIsEnabled"
  },
  {
    "key": "cmd+i",
    "command": "-composer.startComposerPrompt",
    "when": "composerIsEnabled"
  },
  {
    "key": "cmd+9",
    "command": "workbench.action.zoomOut"
  },
  {
    "key": "cmd+-",
    "command": "-workbench.action.zoomOut"
  }
]
